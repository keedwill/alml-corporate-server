<div class="container mt-4">
    

<table class="table">
  <thead>
    <tr>
      <th scope="col">Service ID</th>
      <th scope="col">Service Name</th>
      <th scope="col">Service Description</th>
      <th scope="col">Service Status</th>
      <th scope="col">Service  Fee</th>
      
      <th scope="col">Created At</th>
      <th scope="col">Updated At</th>
      
    </tr>
  </thead>
  <tbody>
     
       
       
        {{#each services }}
            
    <tr>
      <th scope="row">{{this.dataValues.id}}</th>
      <td>{{this.dataValues.name}}</td>
      <td>{{this.dataValues.description}}</td>
      <td>{{this.dataValues.status}}</td>
      <td>{{this.dataValues.fee}}</td>

      <td>{{ formatDate this.dataValues.createdAt 'MMMM Do YYYY, h:mm:ss a'}}</td>
      <td>{{ formatDate this.dataValues.updatedAt 'MMMM Do YYYY, h:mm:ss a'}}</td>
      <td><a href="/admin/services/edit/{{this.dataValues.id}}" class="btn btn-warning">
       <span><i class="fas fa-edit"></i></span>
      Edit Service</a></td>
    </tr>
        {{/each}}   
      
         
     
   
  </tbody>
</table>
</div>